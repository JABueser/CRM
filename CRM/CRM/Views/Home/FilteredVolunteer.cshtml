@model IEnumerable<CRM.Models.Volunteer>

<table>
    <tr>
        <th>Interest</th>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Email</th>
        <th>Phone</th>
    </tr>

    @foreach (CRM.Models.Volunteer r in Model)
    {
        <tr>
            <td>@r.interest</td>
            <td>@r.lastName</td>
            <td>@r.firstName</td>
            <td>@r.email</td>
            <td>@r.phone</td>
        </tr>
    }
</table>

